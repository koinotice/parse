{
  "name": "api-rest-js",
  "version": "0.1.0",
  "description": "CORE API REST",
  "main": "server.js",
  "scripts": {
    "pm2:start": "pm2 start server.js -i max --name=\"api-rest-js\"",
    "pm2:stop": "pm2 stop server.js",
    "start": "node ./server.js",
    "test": "npm run test:cover",
    "code:lint": "eslint \"**/*.js\"",
    "test:unit": "mocha --full-trace --timeout 10000 test/unit/**/*.test.js",
    "test:cover": "nyc mocha --timeout 10000 test/unit/**/*.test.js",
    "test:int": "mocha --full-trace --timeout 10000 test/int/**/*.test.js",
    "build": "npm run code:lint && npm run test:cover && npm run test:int"
  },
  "keywords": [
    "rest",
    "nodejs",
    "js"
  ],
  "engines": {
    "node": ">=8.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/douglaspands/api-rest-js.git"
  },
  "author": "douglaspands",
  "license": "ISC",
  "dependencies": {
    "async": "^2.6.1",
    "async-redis": "^1.1.5",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "eth-block-tracker": "^4.1.0",
    "ethjs-provider-http": "^0.1.6",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "express-healthcheck": "^0.1.0",
    "http-status-codes": "^1.3.0",
    "ioredis": "^4.5.1",
    "is-json": "^2.0.1",
    "is-type-of": "^1.2.1",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "node-json-rpc": "^0.0.1",
    "parse": "^2.2.0",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "truffle-contract": "^4.0.2",
    "truffle-hdwallet-provider": "^1.0.2",
    "truffle-privatekey-provider": "^1.0.0",
    "web3": "^1.0.0-beta.37",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "mock-require": "^3.0.2",
    "nock": "^10.0.4",
    "nyc": "^13.1.0",
    "supertest": "^3.3.0"
  }
}
